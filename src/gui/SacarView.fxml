<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="322.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SacarViewController">
   <children>
      <Label layoutX="112.0" layoutY="25.0" text="Sacar Dinheiro">
         <font>
            <Font name="Arial Black" size="27.0" />
         </font>
      </Label>
      <Label layoutX="62.0" layoutY="88.0" text="Digite o cpf da conta que deseja sacar">
         <font>
            <Font name="Century" size="18.0" />
         </font>
      </Label>
      <Label layoutX="78.0" layoutY="151.0" text="CPF:">
         <font>
            <Font name="Century" size="16.0" />
         </font>
      </Label>
      <Label layoutX="62.0" layoutY="194.0" text="Valor:">
         <font>
            <Font name="Century" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="fildSacar" layoutX="128.0" layoutY="149.0" prefHeight="25.0" prefWidth="251.0" />
      <TextField fx:id="fildValor" layoutX="128.0" layoutY="194.0" prefHeight="25.0" prefWidth="251.0" />
      <Button fx:id="btSacar" layoutX="334.0" layoutY="259.0" mnemonicParsing="false" onAction="#onBtSacar" text="Sacar" />
      <Button fx:id="btCancelar" layoutX="254.0" layoutY="259.0" mnemonicParsing="false" onAction="#onBtCancelar" text="Cancelar" />
   </children>
</AnchorPane>
