<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="252.0" prefWidth="497.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RemoveAccountController">
   <children>
      <Label alignment="CENTER" layoutY="14.0" prefHeight="37.0" prefWidth="497.0" text="Remover Conta">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="77.0" text="Escolha o cpf da conta que deseja remover">
         <font>
            <Font name="Century" size="15.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="118.0" text="CPF:">
         <font>
            <Font name="Century" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="fildCPF" layoutX="100.0" layoutY="114.0" prefHeight="25.0" prefWidth="243.0" />
      <Button fx:id="btRemover" layoutX="50.0" layoutY="180.0" mnemonicParsing="false" onAction="#onButtonRemover" text="Remover" />
      <Button fx:id="btCancelar" layoutX="134.0" layoutY="180.0" mnemonicParsing="false" onAction="#onButtonCancel" text="Cancelar" />
   </children>
</AnchorPane>
